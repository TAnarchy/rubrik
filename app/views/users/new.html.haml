%h1
  New user
- form_for(@user) do |f|
  = f.error_messages
  %p
    = f.label :username
    %br
    = f.text_field :username
  %p
    = f.label :password
    %br
    = f.password_field :password
  %p
    = f.label :password_confirmation
    %br
    = f.password_field :password_confirmation
  %p
    = f.label :email
    %br
    = f.text_field :email
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.submit 'Create'

= link_to 'Back', users_path
